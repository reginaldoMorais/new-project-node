{
  "presets": ["es2015", "react"],
  "plugins": [
    "transform-object-rest-spread",
    ["module-alias", [
      { "src": "./node_modules", "expose": "Modules" },
      { "src": "./public", "expose": "Assets" },
      { "src": "./config", "expose": "Config" },
      { "src": "./src/views/templates", "expose": "Templates" }
    ]]
  ]
}
